    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "port": 3000,
            //"program": "${workspaceFolder}\\src\\cribbage\\app.js"
            "program": "${workspaceFolder}\\bin\\www"
        },
        {
            "type": "node",
            "request": "attach",
            "cwd": "${workspaceFolder}",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}